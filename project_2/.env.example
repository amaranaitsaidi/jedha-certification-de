# ============================================
# AIRFLOW CONFIGURATION
# ============================================
AIRFLOW_INITIAL_ADMIN_USER=admin
AIRFLOW_INITIAL_ADMIN_PASSWORD=admin

# ============================================
# POSTGRESQL (Amazon Database)
# ============================================
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin123
POSTGRES_DB=amazon_db
POSTGRES_HOST=amazon_postgres_db
POSTGRES_PORT=5432

# ============================================
# AIRFLOW POSTGRES DATABASE
# ============================================
POSTGRES_USER_AIRFLOW=airflow
POSTGRES_PASSWORD_AIRFLOW=airflow
POSTGRES_DB_AIRFLOW=airflow

# ============================================
# AWS S3 CONFIGURATION
# ============================================
AWS_ACCESS_KEY=your_aws_access_key
AWS_SECRET_KEY=your_aws_secret_key
AWS_REGION=eu-west-3
AWS_S3_BUCKET=your-bucket-name

# ============================================
# MONGODB CONFIGURATION
# ============================================
MONGO_USER=admin
MONGODB_PASSWORD=changeme
MONGODB_HOST=mongodb
MONGODB_PORT=27017
MONGODB_CONNECTION_STRING=mongodb://admin:changeme@mongodb:27017/

# ============================================
# SNOWFLAKE CONFIGURATION
# ============================================
SNOWFLAKE_USER=your_snowflake_user
SNOWFLAKE_PASSWORD=your_snowflake_password
SNOWFLAKE_ACCOUNT=your_account_identifier
SNOWFLAKE_DB=DB_AMZ
SNOWFLAKE_SCHEMA=REVIEW
SNOWFLAKE_WH=COMPUTE_WH
SNOWFLAKE_ROLE=SYSADMIN

# ============================================
# AIRFLOW CONNECTIONS (Pre-configured)
# ============================================
# PostgreSQL Amazon Database Connection
AIRFLOW_CONN_PG_AMAZON=postgresql://admin:admin123@amazon_postgres_db:5432/amazon_db

# AWS S3 Connection (URL-encoded special characters)
AIRFLOW_CONN_AWS_S3_DEFAULT=aws://your_aws_key:your_aws_secret@?region_name=eu-west-3&bucket_name=your-bucket-name

# MongoDB Connection
AIRFLOW_CONN_MONGO=mongodb://admin:changeme@mongodb:27017/

# Snowflake Connection (URL-encoded special characters like ; = %3B)
AIRFLOW_CONN_SNOWFLAKE_CONN=snowflake://your_user:your_password@your_account/DB_AMZ/REVIEW?warehouse=COMPUTE_WH&role=SYSADMIN

